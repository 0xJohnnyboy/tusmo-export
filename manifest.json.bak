{
    "manifest_version": 3,
    "name": "Tusmo Export",
    "version": "1.1",
    "description": "Export the leaderboard from tusmo.xyz",
    "permissions": [
        "scripting",
        "storage",
        "activeTab"
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "favicon.ico"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.tusmo.xyz/*"
            ],
            "js": [
                "inject-via-scripting.js"
            ],
            "run_at": "document_start"
        }
    ],
    "host_permissions": [
        "https://tusmo.xyz/*"
    ]
}